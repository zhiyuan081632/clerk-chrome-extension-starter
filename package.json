{
  "name": "clerk-chrome-extension-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@clerk/chrome-extension": "^0.3.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.12",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "babel-preset-minify": "^0.5.2",
    "cross-env": "^7.0.3",
    "parcel-bundler": "^1.12.5",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:app": "cross-env INLINE_RUNTIME_CHUNK=false react-scripts build",
    "build:options": "parcel build src/options.tsx --out-dir build/static/js --out-file options.js --no-minify",
    "build": "npm run build:app && npm run build:options",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
